    <meta name="description" content="">
    <meta name="author" content="">

    <title>{% block title %}Mohamed Ali CV {% endblock %} - FrontOffice</title>

    {% block stylesheets %}
        <!-- Bootstrap Core CSS -->
        <link href="{{ asset('/bundles/frontoffice/css/bootstrap.min.css') }}" rel="stylesheet">

        <!-- Custom CSS -->
        <link href="{{ asset('/bundles/frontoffice/css/agency.css') }}" rel="stylesheet">

        <!-- Custom Fonts -->
        <link href="{{ asset('/bundles/frontoffice/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css">
