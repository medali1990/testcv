    <script src="{{ asset('/bundles/frontoffice/js/jquery.js') }}"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="{{ asset('/bundles/frontoffice/js/bootstrap.min.js') }}"></script>

    <!-- Plugin JavaScript -->
    <script src="{{ asset('http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js') }}"></script>
    <script src="{{ asset('/bundles/frontoffice/js/classie.js') }}"></script>
    <script src="{{ asset('/bundles/frontoffice/js/cbpAnimatedHeader.js') }}"></script>

    <!-- Contact Form JavaScript -->
    <script src="{{ asset('/bundles/frontoffice/js/jqBootstrapValidation.js') }}"></script>
    <script src="{{ asset('/bundles/frontoffice/js/contact_me.js') }}"></script>

    <!-- Custom Theme JavaScript -->
    <script src="{{ asset('/bundles/frontoffice/js/agency.js') }}"></script>
{% endblock %}
